    "keywords": ["silverstripe", "admin", "contacts", "management"],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "ilateral",
        "email": "info@ilateral.co.uk"
    }],
    "require": {
        "silverstripe/framework": "~3.1",
        "colymba/gridfield-bulk-editing-tools": "~2.1",
        "silverstripe/tagfield": "~1.0.0"
    },
    "replace": {
        "silverstripe/contacts": "self.version"
    },
    "extra": {
        "installer-name": "contacts"
