{
    "name": "i-lateral/silverstripe-contacts",
    "description": "Contact management module, allowing import, export, management, grouping and tagging of contacts",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/i-lateral/silverstripe-contacts",
    "keywords": [
        "silverstripe",
        "admin",
        "contacts",
        "management"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "ilateral",
        "email": "info@ilateral.co.uk"
    }],
    "require": {
        "silverstripe/framework": "^4.0",
        "colymba/gridfield-bulk-editing-tools": "*",
        "silverstripe/tagfield": "^2.0"
    },
    "replace": {
        "silverstripe/contacts": "self.version"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\Admin\\": "code/",
            "SilverStripe\\Admin\\Tests\\": "tests/php/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
